{
    "service": {
        "id": "01",
        "path": "/static"
    },
    "services": {
        "api": "https://api.alexlipianu.com"
    },
    "cookies": {
        "analytics": {
            "name": "alipianu-analytics",
            "maxAge": 100000,
            "length": 20
        }
    },
    "core": {
        "service": {
            "port": 3002,
            "corsOptions": {
                "origin": "https://alexlipianu.com",
                "methods": "GET,POST,OPTIONS",
                "credentials": true,
                "allowedHeaders": "Content-Type, Authorization, X-Requested-With, Accept, Origin"
            },
            "database": {
                "url": "mongodb://localhost:27017",
                "name": "alexlipianu-com",
                "options": {
                    "useNewUrlParser": true
                }
            }
        }
    }
}
